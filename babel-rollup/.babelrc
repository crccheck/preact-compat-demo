{
  "plugins": [
    ["transform-react-jsx", { "pragma":"h" }],
      ["module-resolver", {
      "root": ["."],
      "alias": {
        "react": "preact-compat",
        "react-dom": "preact-compat",
        "create-react-class": "preact-compat/lib/create-react-class"
      }
    }],
    "external-helpers"
  ],
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "react"
  ]
}
